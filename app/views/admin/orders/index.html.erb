<div class="page-header">
  <h1>依頼一覧</h1>
</div>
<table>
  <tr>
    <th>id</th>
    <th>first_name</th>
    <th>last_name</th>
    <th>day</th>
    <th>location</th>
    <th>phone_number</th>
    <th>comment</th>
    <th>created_at</th>
  </tr>
  <% @orders.each do |order| %>
  <tr>
    <td><%= order.id %></td>
    <td><%= order.first_name %></td>
    <td><%= order.last_name %></td>
    <td><%= order.day %></td>
    <td><%= order.location %></td>
    <td><%= order.phone_number %></td>
    <td><%= order.comment %></td>
    <td><%= order.created_at %></td>
  </tr>
  <% end %>
</table>
